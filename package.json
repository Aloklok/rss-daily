{
  "name": "briefing-hub",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo -H 127.0.0.1",
    "build": "next build",
    "start": "next start -H 127.0.0.1",
    "lint": "eslint . --quiet",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:report": "vitest run",
    "test:e2e": "env -u HTTP_PROXY -u HTTPS_PROXY -u ALL_PROXY playwright test",
    "test:e2e:smoke": "env -u HTTP_PROXY -u HTTPS_PROXY -u ALL_PROXY playwright test e2e/tests/home.spec.ts",
    "format": "prettier --write .",
    "analyze": "cross-env ANALYZE=true next build --webpack",
    "vercel": "NODE_OPTIONS='--dns-result-order=ipv4first' vercel dev",
    "gen:supabase-types": "supabase gen types typescript --project-id rpctclawjqruwpfirkez > src/shared/types/generated/supabase.ts",
    "gen:freshrss-types": "npx openapi-typescript src/shared/types/generated/freshrss.yaml -o src/shared/types/generated/freshrss.ts",
    "prompt:pull": "npx tsx scripts/pull-prompt.ts",
    "prompt:push": "npx tsx scripts/push-prompt.ts",
    "chat-prompt:pull": "npx tsx scripts/pull-chat-prompt.ts",
    "chat-prompt:push": "npx tsx scripts/push-chat-prompt.ts",
    "search:backfill": "npx tsx scripts/backfill-embeddings.ts",
    "search:update": "npx tsx scripts/update-search-rpc.ts",
    "translate:backfill": "npx tsx scripts/backfill-translations.ts",
    "prepare": "husky"
  },
  "browserslist": [
    "last 2 versions",
    "> 0.5%",
    "not dead",
    "not IE 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.19",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "dayjs": "^1.11.19",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sanitize-html": "^2.17.0",
    "server-only": "^0.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@next/bundle-analyzer": "^16.1.3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.9",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.0.0",
    "@types/sanitize-html": "^2.16.0",
    "@vercel/node": "^5.5.15",
    "@vitest/browser": "^4.0.16",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "node-fetch": "^3.3.2",
    "openapi-typescript": "^7.10.1",
    "playwright": "^1.57.0",
    "postcss": "^8.4.38",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.16"
  }
}